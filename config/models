User
    ident Text
    password Text Maybe
    isAdmin Bool
    UniqueUser ident
    deriving Typeable
Email
    email Text
    user UserId Maybe
    verkey Text Maybe
    UniqueEmail email
Item
    name Text
    price Int
Transaction
    items [Item]
    totalPrice Int
